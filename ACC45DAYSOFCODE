#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int count=0,c=0,a[7];
	    for(int i=0;i<7;i++){
	        cin>>a[i];
	        if(a[i]==1)
	        {count++;}
	        if(a[i]==0)
	       { c++;}
	    }
	    if(count>c)
	   { cout<<"yes"<<endl;}
	    else{
	    cout<<"no"<<endl;}
	    
	}
	return 0;
}
